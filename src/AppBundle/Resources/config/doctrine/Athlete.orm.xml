<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Athlete" table="athlete">
    <indexes>
      <index name="FK_CategoryID2" columns="category"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstname" type="string" column="firstname" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastname" type="string" column="lastname" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="category" target-entity="Category" fetch="LAZY">
      <join-columns>
        <join-column name="category" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
